  "scripts": {
    "quasar": "quasar",
    "dev": "quasar dev",
    "build": "quasar build --mode spa",
    "dep-update": "npx -p npm-check-updates ncu -u && npm install",
    "lint": "eslint --ext .js,.vue src",
